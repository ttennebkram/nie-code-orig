<onebox type="external" suppressDateTime="false" suppressIPAddr="false">
  <name>SearchTrack_Suggestions</name>
  <security userAuth="none" />
  <description>Provide predefined suggestions for certain search terms, defined by the administrator.</description>
  <trigger triggerType="null" />
  <!-- NOT
  <providerURL>http://cp1.ideaeng.net:9000/onebox</providerURL>
  -->
  <providerURL>http://cable.ideaeng.net:9000/onebox</providerURL>
  <resultsTemplate>
    <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="suggestions">
      <xsl:for-each select="MODULE_RESULT">
	<!-- This template must align with XML generated by
		nie/sn/SnURLRecord.generateWebmasterSuggestsGoogleOneBoxItem()
	-->
        <a>
          <xsl:attribute name="href">
            <xsl:value-of select="U" />
          </xsl:attribute>
          <b>
            <xsl:value-of select="Field[@name='title']" />
          </b>
          <br />
        </a>
        <small>
          <xsl:value-of select="Field[@name='summary']" />
        </small>
        <p />
      </xsl:for-each>
    </xsl:template>
  </resultsTemplate>
</onebox>
