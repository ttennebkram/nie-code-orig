			]]>
		</marker_text>
		-->
    <!--
		<surround_template>
			http://ideaeng.com/search/sr-template.html
		</surround_template>
		<marker_text replace="TRUE">
			<![CDATA[
				search reports go here
			]]>
		</marker_text>
		-->
    <reports_directory>myreports</reports_directory>
    <!--
			report dir =
			css =
			report menu definitiohn =
		-->
  </report_settings>
</search_tracking>