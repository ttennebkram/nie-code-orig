<table name="nie_map_site_assoc">

<field name="map_id"
	type="int"
	cgi_field_name="nie_map_id"
	is_not_null="TRUE"
	is_indexed="TRUE"
/>
<field name="site_id"
	type="int"
	cgi_field_name="nie_site_id"
	is_not_null="TRUE"
	is_indexed="TRUE"
/>

<field name="is_active"
	type="boolean"
	is_indexed="TRUE"
	description="Is this the site we should assume of no others are specified"
/>

<field name="created_date"
	type="timestamp"
/>
<field name="created_by_person"
	type="text"
	size="64"
/>
<field name="created_comment"
	type="text"
	size="1024"
/>

<field name="last_edit_date"
	type="timestamp"
/>
<field name="last_edit_by_person"
	type="text"
	size="64"
/>
<field name="last_edit_comment"
	type="text"
	size="1024"
/>

</table>
